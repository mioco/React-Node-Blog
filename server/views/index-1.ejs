<%- include header %>
<% posts.forEach(function(post, index) { %>
  <p><h2><a href="article/<%= post._id%>"><%= post.title %></a></h2></p>
  <p class='info'>
  	日期: <%= post.time.minute %>
    标签: 
    <% post.tags.forEach(function(tag, index) { %>
      <% if (tag) { %>
        <a href="/tags/<%= tag %>"><%= tag %></a>
      <% } %>
    <% }) %>
  </p>
  <p><%- post.post %></p>
<% }) %>
<%- include paging %>
<%- include footer %>